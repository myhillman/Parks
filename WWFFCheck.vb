    Private Sub CheckBox1_CheckedChanged(sender As Object, e As EventArgs) Handles CheckBox1.CheckedChanged
        If CType(sender, CheckBox).Checked Then
            Me.CheckBoxAll.Checked = False      ' uncheck All if option selected
        End If
    End Sub

